&i2c3 {
	oplus_sc6607_ufcs:oplus,sc6607_ufcs@63 {
		status = "okay";
		compatible = "oplus,sc6607-ufcs";
		reg = <0x63>;
	};
};

&odm {
	oplus_ufcs_protocol: oplus,ufcs_protocol {
		status = "ok";
		compatible = "oplus,ufcs-protocol";
	};
};

